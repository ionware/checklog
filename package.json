        "react-dropdown": "^1.4.0",
        "react-notifications": "^1.4.3",
        "react-redux": "^5.0.7",
        "react-redux-loading-bar": "^4.0.3",
        "react-router-dom": "^4.2.2",
        "react-select": "^1.2.1",
        "react-topbar-progress-indicator": "^2.0.0",
        "redux": "^3.7.2"
